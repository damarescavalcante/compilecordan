<?xml version="1.0" encoding="utf-8"?>
<manifest
    xmlns:android="http://schemas.android.com/apk/res/android"
    package="damarescavalcante.compilecordan">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_html"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <!-- main activity -->
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>


        <!-- html activity -->
        <activity
            android:name=".SecondLayoutHtml"
            android:label="@string/lbl_text_html">
        </activity>


        <!-- css activity -->
        <activity
            android:name=".ThirdLayoutCss"
            android:label="@string/lbl_text_css">
        </activity>

        <!-- bootstrap activity -->
        <activity
            android:name=".FourthLayoutBootstrap"
            android:label="@string/lbl_text_bootstrap">
        </activity>
    </application>

</manifest>
